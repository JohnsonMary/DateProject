package Practice_day59;

import java.util.*;



public class ArrayListExample {
	public static void main(String[] args) {
		
		List <Integer> col = new ArrayList<>();
		
		col.add(34);
		col.add(4);
		col.add(55);
		col.add(87);
		col.add(43);

		Iterator <Integer> iterator = col.iterator();
		
		System.out.println(iterator.next());
		System.out.println(iterator.next());
		System.out.println(iterator.next());
		
		while(iterator.hasNext()) {
			
			if(iterator.next()>60) {
				iterator.remove();
				
			}
			
		} System.out.println(col);
		
		
		
		List <String> fruits = new LinkedList<>();
		
		fruits.add("Apple");
		fruits.add("Banana");
		fruits.add("Pear");
		fruits.add("Orange");
		
		System.out.println(fruits);
		
		Iterator <String> iterator2 = fruits.iterator();
		
		
		while(iterator2.hasNext()) {
			
			String target = iterator2.next();
			if(target.contains("a") && target.contains("e") ) {
				
				iterator2.remove();	
			}		
		}
		    System.out.println(fruits);

		    Set <Integer> numSet = new HashSet<>();
		    numSet.add(64);
		    numSet.add(2);
		    numSet.add(2);
		    numSet.add(2);
		    numSet.add(34);
		    
		    System.out.println(numSet);
		    
		    Iterator <Integer> iterator3 = numSet.iterator();
		    
		    while (iterator3.hasNext()) {
		    	
		    	System.out.println(iterator3.next());
		    	
		    }System.out.println(numSet);
		    
		    
		    Collections.sort(col);
		    System.out.println(col);

		    List <Integer> list = new ArrayList<>();
		    
		    list.add(20);
		    list.add(43);
		    list.add(43);
		    list.add(43);
		    list.add(84);
		    list.add(99);
		    list.add(43);
		    list.add(23);
		    
		    System.out.println(list);
		    
		    List <Integer> list2 = list.subList(2, 6);
		    System.out.println(list2);
		    list2.set(3, 25);
		    list2.set(2, 85);
		    System.out.println(list2);
		    System.out.println(list);

		    System.out.println(Collections.frequency(list2, 43));
		    
		    
		
		
		
		
		

		
		
		
		
		
		
		
		
		
		
	}

}
